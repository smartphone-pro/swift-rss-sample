target 'RSSReader' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RSSReader
  pod 'SVProgressHUD'
  pod 'MWFeedParser'
  pod 'KINWebBrowser'
  pod 'AFNetworking'
  
  target 'RSSReaderTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
